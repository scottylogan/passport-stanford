    "body-parser": "*",
    "cookie-parser": "*",
    "errorhandler": "*",
    "express": "*",
    "express-session": "*",
    "method-override": "*",
    "morgan": "*",
    "passport": "*",
    "passport-saml": "*",
    "passport-stanford": "file:..",
    "pug": "3.0.1",
    "qs": "*"
  },
  "engines": {
    "node": ">= 8.0.0"
  }
}
